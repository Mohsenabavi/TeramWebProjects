@using Teram.Web.Core.Page;

@model List<Navigation>
<nav class="navbar navbar-expand-lg navbar-light bg-white my-2">
	<div class="container-fluid">
		<button class="navbar-toggler px-0" type="button" data-bs-toggle="collapse"
				data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false"
				aria-label="Toggle navigation">
			<img src="~/Themes/OnlineDaewoo/Icons/menu.svg" />
			<span>منو</span>
		</button>
	<div class="collapse navbar-collapse" id="navbarNav">
		<ul class="navbar-nav">

			@foreach (var item in Model)
			{
				<li class="nav-item">
					<a href="@item.Url" target="@item.Target" class="nav-link">@item.Title</a>
				</li>
			}
		</ul>
	</div>
	</div>
</nav>

