@using Teram.Web.Core.Helper

@inject IStringLocalizer<Teram.HR.Module.Recruitment.Controllers.ViewRegisteredPeopleByDayChartController> localizer
@{
    ViewData["Title"] = localizer["ViewRegisteredPeopleByDayChart"];
}
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="card card-primary card-outline border-info">
                    <div class="card-header bg-dark text-light p-1">
                        <h5 class="card-title mt-2 mr-2"><i class=" text-info"></i> @localizer["ViewRegisteredPeopleByDayChart"]</h5>
                       
                    </div>
                    <input type="hidden" value="@Model.ModuleId" id="ViewRegisteredPeopleByDayChartComponentId" />
                    <div class="card-body manageForm">
                        <figure class="highcharts-figure">
                            <div id="GetRegisteredPeopleByDayChartComponent" class=container></div>
                            <p class="highcharts-description">
                            </p>
                        </figure>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@Html.AddResource(PageResourceType.CSS, @<link href="/ExternalModule/HR/Module/Recruitment/Style/HighChart.css" rel="stylesheet" />)
@Html.AddResource(PageResourceType.JavaScript, @<script src="~/ExternalModule/HR/Module/Recruitment/Scripts/ViewRegisteredPeopleByDayChart.js" type="text/javascript"></script>)
@Html.AddResource(PageResourceType.JavaScript, @<script src="~/lib/highcharts/highcharts.js" type="text/javascript"></script>)
@Html.AddResource(PageResourceType.JavaScript, @<script src="~/lib/highcharts/modules/exporting.js" type="text/javascript"></script>)
@Html.AddResource(PageResourceType.JavaScript, @<script src="~/lib/highcharts/modules/export-data.js" type="text/javascript"></script>)
@Html.AddResource(PageResourceType.JavaScript, @<script src="~/lib/highcharts/modules/accessibility.js" type="text/javascript"></script>)